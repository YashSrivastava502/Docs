Enable-WindowsOptionalFeature -Online -FeatureName Microsoft-Hyper-V -All -NoRestart
Restart-Computer


# Silent install of Provider
Start-Process "C:\Installers\ASRProvider.exe" "/q /x:C:\Installers\ASR" -Wait

# Register with Vault
& "C:\Program Files\Microsoft Azure Site Recovery Provider\DRConfigurator.exe" `
   /register `
   /VaultCredentialFile:"C:\Installers\VaultCredentials.vaultcredentials"



   Start-Process "C:\Installers\MARSAgentInstaller.exe" "/q" -Wait

   
